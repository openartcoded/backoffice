{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "version": 1,
    "newProjectRoot": "projects",
    "projects": {
        "artcoded-backoffice": {
            "projectType": "application",
            "schematics": {
                "@schematics/angular:component": {
                    "style": "scss",
                    "skipTests": true
                }
            },
            "root": "",
            "sourceRoot": "src",
            "prefix": "app",
            "architect": {
                "build": {
                    "builder": "@angular/build:application",
                    "options": {
                        "allowedCommonJsDependencies": [
                            "highlight.js"
                        ],
                        "outputPath": {
                            "base": "dist/artcoded-backoffice"
                        },
                        "index": "src/index.html",
                        "polyfills": [
                            "src/polyfills.ts",
                            "@angular/localize/init"
                        ],
                        "tsConfig": "tsconfig.app.json",
                        "assets": [
                            "src/favicon.ico",
                            "src/robots.txt",
                            "src/assets",
                            {
                                "glob": "**/*",
                                "input": "node_modules/ng2-pdfjs-viewer/pdfjs",
                                "output": "/assets/pdfjs"
                            },
                            {
                                "glob": "**/*.wasm",
                                "input": "node_modules/@nbittich/web-rdfa",
                                "output": "/assets/wasm"
                            },
                            "src/manifest.webmanifest"
                        ],
                        "styles": [
                            "node_modules/primeicons/primeicons.css",
                            "node_modules/@zazuko/yasgui/build/yasgui.min.css",
                            "node_modules/bootstrap/dist/css/bootstrap.min.css",
                            "node_modules/prismjs/themes/prism.css",
                            "node_modules/bootstrap-markdown/css/bootstrap-markdown.min.css",
                            "src/styles.scss"
                        ],
                        "scripts": [
                            "node_modules/prismjs/prism.js",
                            "node_modules/prismjs/components/prism-css.min.js",
                            "node_modules/prismjs/components/prism-javascript.min.js",
                            "node_modules/prismjs/components/prism-typescript.min.js",
                            "node_modules/prismjs/components/prism-java.min.js",
                            "node_modules/prismjs/components/prism-yaml.min.js",
                            "node_modules/prismjs/components/prism-turtle.min.js",
                            "node_modules/prismjs/components/prism-json.min.js",
                            "node_modules/prismjs/components/prism-markup.min.js",
                            "node_modules/jquery/dist/jquery.js",
                            "node_modules/bootstrap-markdown/js/bootstrap-markdown.js"
                        ],
                        "extractLicenses": false,
                        "sourceMap": true,
                        "optimization": false,
                        "namedChunks": true,
                        "browser": "src/main.ts"
                    },
                    "configurations": {
                        "production": {
                            "fileReplacements": [
                                {
                                    "replace": "src/environments/environment.ts",
                                    "with": "src/environments/environment.prod.ts"
                                }
                            ],
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "namedChunks": true,
                            "extractLicenses": true,
                            "budgets": [
                                {
                                    "type": "initial",
                                    "maximumWarning": "7mb",
                                    "maximumError": "10mb"
                                },
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "6kb",
                                    "maximumError": "10kb"
                                }
                            ],
                            "serviceWorker": "ngsw-config.json"
                        }
                    },
                    "defaultConfiguration": ""
                },
                "serve": {
                    "builder": "@angular/build:dev-server",
                    "options": {
                        "proxyConfig": "src/proxy.conf.json",
                        "buildTarget": "artcoded-backoffice:build"
                    },
                    "configurations": {
                        "production": {
                            "buildTarget": "artcoded-backoffice:build:production"
                        }
                    }
                },
                "extract-i18n": {
                    "builder": "@angular/build:extract-i18n",
                    "options": {
                        "buildTarget": "artcoded-backoffice:build"
                    }
                },
                "lint": {
                    "builder": "@angular-eslint/builder:lint",
                    "options": {
                        "lintFilePatterns": [
                            "src/**/*.ts",
                            "src/**/*.html"
                        ]
                    }
                }
            }
        }
    },
    "cli": {
        "schematicCollections": [
            "@angular-eslint/schematics"
        ],
        "analytics": false
    }
}
